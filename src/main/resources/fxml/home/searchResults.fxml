<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="home.SearchResultsController">
   <children>
      <JFXListView fx:id="list_view" onMouseClicked="#listViewClicked" prefHeight="952.0" prefWidth="480.0" />
      <HBox alignment="TOP_RIGHT" focusTraversable="true" pickOnBounds="false">
         <children>
            <JFXButton contentDisplay="GRAPHIC_ONLY" onAction="#closeDrawer" prefHeight="35.0" prefWidth="35.0" style="-fx-background-color: F1F1F1;" styleClass="exit-button">
               <graphic>
                  <FontAwesomeIconView glyphName="CLOSE" size="24" styleClass="request-glyph" />
               </graphic>
               <HBox.margin>
                  <Insets right="18.0" top="5.0" />
               </HBox.margin>
            </JFXButton>
         </children>
      </HBox>
   </children>
</StackPane>
